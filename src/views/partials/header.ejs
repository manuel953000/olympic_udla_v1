  <script >
 
    /* Determinamos el tiempo total en segundos */
    var totalTiempo=10;
    /* Determinamos la url del archivo o del boton de descarga,(en ejemplo descarga de freebsd */
    var url="https://www.youtube.com/watch?v=4iex7arlWrE";
 
    function updateReloj()
    {
        document.getElementById('CuentaAtras').innerHTML = totalTiempo+" segundos";
 
        if(totalTiempo==0)
        {
            document.getElementById('CuentaAtras').innerHTML = "Se acabo el tiempo, buena suerte !";
        }else{
            /* Restamos un segundo al tiempo restante */
            totalTiempo-=1;
            /* Ejecutamos nuevamente la función al pasar 1000 milisegundos (1 segundo) */
            setTimeout("updateReloj()",1000);
        }
    }
 
    window.onload=updateReloj;
 
    </script>
    <!--Scripts cuenta regresiva-->
  

